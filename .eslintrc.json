{
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-unused-vars": 1,
    "no-console": 0,
    "no-undef": 0,
    "no-useless-escape": 0,
    "no-with": 0
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
